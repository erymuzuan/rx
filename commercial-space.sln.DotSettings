	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=3488FCEE3861D4488CFB1996B3B5D415/Text/@EntryValue"> var tcs = new $.Deferred();&#xD;
var data = $END$;&#xD;
isBusy(true);&#xD;
&#xD;
context.post(data, "$URL$")&#xD;
	.then(function (result) {&#xD;
		isBusy(false);&#xD;
&#xD;
&#xD;
		tcs.resolve(result);&#xD;
	});&#xD;
return tcs.promise();</s:String>
