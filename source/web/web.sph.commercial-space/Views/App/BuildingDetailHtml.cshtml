@model Bespoke.SphCommercialSpaces.Domain.BuildingTemplate
@{
    Layout = null;
}
<div id="building-detail-page">
    <h1>@Model.FormDesign.Name</h1>
    <span>@Model.FormDesign.Description</span>
    <form class="form-horizontal" id="building-form" data-bind="with: building">
        @foreach (var fe in Model.FormDesign.FormElementCollection)
        {
            @Html.EditorFor(m => fe)
        
        }
    </form>

    <section class="modal hide google-maps" id="map-panel">

        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h2>Peta bangunan</h2>
        </div>
        <div class="modal-body" style="width: 900px; height: 500px; max-height: 500px">
            <div id="map" style="width: 900px; height: 500px"></div>
        </div>

        <div class="modal-footer">
            <input data-dismiss="modal" type="button" class="btn btn-primary" value="save map" data-bind="click: saveMapCommand" />
            <a href="#" class="btn" data-dismiss="modal">Cancel</a>
        </div>
    </section>
</div>
