@model Bespoke.SphCommercialSpaces.Domain.NumberTextBox
<div class="control-group">
    <label for="@Model.Path" class="control-label">@Model.Label</label>
    <div class="controls">
        <input step="@Model.Step" class="@Model.CssClass" title="@Model.Tooltip" data-bind="value: @Model.Path" id="@Model.Path" type="number" name="@Model.Path" />
    </div>
</div>  