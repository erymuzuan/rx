@model Bespoke.SphCommercialSpaces.Domain.CustomListDefinitionElement


<div>
    <input type="button" class="btn pull-right" value="@Model.Label" />
    <table class="table table-condensed table-striped">
        <thead>
            <tr>
            @foreach (var c in Model.CustomFieldCollection)
            {
                <th>@c.Name</th>
            }
            </tr>
        </thead>
        <tbody>
            <tr>
                 @foreach (var c in Model.CustomFieldCollection)
                       {
                           <td>
                               <input class="input-small" type="text" data-bind2="value: Number, uniqueName: true" value="G" />

                           </td>
                       }

            </tr>
        </tbody>
    </table>
</div>
