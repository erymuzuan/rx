@model Bespoke.SphCommercialSpaces.Domain.BuildingElement

<!--ko if:$data['$type']() === "Bespoke.SphCommercialSpaces.Domain.BuildingElement, domain.commercialspace" -->
<div data-bind="css: { 'selected-form-element': isSelected }, click: $root.selectFormElement">
    @Html.Partial("_DesignerContextAction")
    <label data-bind="text: Label" class="control-label">LABEL</label>
    <div class="controls">
        <select data-bind="attr: { 'class': CssClass() + ' ' + Size(), 'title': Tooltip }"></select>
        <span data-bind="text: HelpText" class="help-inline"></span>
    </div>
</div>
<!--/ko-->
