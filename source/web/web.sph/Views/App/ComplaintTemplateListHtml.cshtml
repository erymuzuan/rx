@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <h1 class="col-lg-12">Templat Aduan</h1>
</div>

<table class="table table-striped" data-bind="filter : {path:'tbody>tr'}">
    <thead>
        <tr>
            <th>Nama</th>
            <th>Keterangan</th>
            <th>Aktif</th>
        </tr>
    </thead>
    <tbody data-bind="foreach:templates">
        <tr>
            <td>
                <a data-bind="attr: { href: '/#/template.complaint-id.0/' + ComplaintTemplateId() }, text: Name"></a>
            </td>
            <td data-bind="text: FormDesign().Description"></td>
            <td>
                <input type="checkbox" data-bind="checked:IsActive" />
            </td>
        </tr>
    </tbody>
</table>