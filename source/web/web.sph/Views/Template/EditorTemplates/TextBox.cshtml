@model Bespoke.Sph.Domain.TextBox

<!--ko if: $data['$type']() === "Bespoke.Sph.Domain.TextBox, domain.sph" -->
<div class="form-group">
    <label for="form-element-textbox-minlength" class="control-label">Min length</label>
    <div class="controls">
        <input class="form-control" data-bind="value: MinLength" id="form-element-textbox-minlength" type="text" name="MinLength" />
    </div>
</div>

<div class="form-group">
    <label for="form-element-textbox-maxlength" class="control-label">Max length</label>
    <div class="controls">
        <input class="form-control" data-bind="value: MaxLength" id="form-element-textbox-maxlength" type="text" name="MaxLength" />
    </div>
</div>
<!--/ko-->
