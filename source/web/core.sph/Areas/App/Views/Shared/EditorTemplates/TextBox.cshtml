@model Bespoke.Sph.Domain.TextBox

<!--ko if: ko.unwrap($type) === "Bespoke.Sph.Domain.TextBox, domain.sph" -->
<div class="form-group">
    <label for="form-element-textbox-minlength">Autocompletion Entity</label>
    <select class="form-control" data-bind="value: AutoCompletionEntity,
                options : $root.entityOptions,
                optionsValue : 'value',
                optionsText : 'text',
                optionsCaption: '[Select Entity]'" id="form-element-textbox-entity" name="entity"></select>
</div>
<div class="form-group">
    <label for="form-element-textbox-minlength">Autocompletion Field</label>
    <input class="form-control" data-bind="value: AutoCompletionField,entityTypeaheadPath : AutoCompletionEntity" id="form-element-textbox-Field" type="text" name="Field" />
</div>
<div class="form-group">
    <label for="form-element-textbox-minlength">Autocompletion Query</label>
    <input class="form-control" data-bind="value: AutoCompletionQuery" id="form-element-textbox-Query" type="text" name="Query" />
</div>
<div class="form-group">
    <label for="form-element-textbox-minlength">Min length</label>
    <input class="form-control" data-bind="value: MinLength" id="form-element-textbox-minlength" type="text" name="MinLength" />
</div>

<div class="form-group">
    <label for="form-element-textbox-maxlength">Max length</label>
    <input class="form-control" data-bind="value: MaxLength" id="form-element-textbox-maxlength" type="text" name="MaxLength" />
</div>
<!--/ko-->
