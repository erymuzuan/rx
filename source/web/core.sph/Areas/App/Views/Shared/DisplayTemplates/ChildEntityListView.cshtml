
@using System.Web.Mvc.Html
@model Bespoke.Sph.Domain.ChildEntityListView


<!--ko if:ko.unwrap($type) === "Bespoke.Sph.Domain.ChildEntityListView, domain.sph" -->
<div data-bind="css: { 'selected-form-element': isSelected },click: $root.selectFormElement">
    @Html.Partial("_DesignerContextAction")

    <h3 data-bind="text: Label"></h3>
    <table class="table table-condensed table-striped">
        <thead>
            <tr data-bind="foreach: ViewColumnCollection">
                <th>
                    <a href="#" >
                        <span data-bind="text:Header"></span>
                        <i class="fa fa-check"></i>

                    </a>
                </th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < 5; i++)
            {
                <tr data-bind="foreach: ViewColumnCollection">
                    <td>
                        Sample
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<!--/ko-->
