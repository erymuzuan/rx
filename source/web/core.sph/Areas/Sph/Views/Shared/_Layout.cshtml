@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - SPH</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @*@Styles.Render("~/Content/css")*@

    <link href="~/Content/__css.min.css" rel="stylesheet" />

    @Scripts.Render("~/scripts/vendor")
    @Scripts.Render("~/scripts/core")
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
</head>
    <body>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="container">
                @RenderBody()
            </section>
        </div>



        <script src="~/kendo/js/kendo.all.js"></script>
        <script src="~/App/objectbuilders.js" type="text/javascript"></script>
        <script src="~/App/durandal/amd/require.js" type="text/javascript"></script>
        <script type="text/javascript">

            var bespoke = bespoke || {};
            bespoke.sph = bespoke.sph || {};
            bespoke.sph.domain = bespoke.sph.domain || {};


            require.config({
                baseUrl: "/App",
                waitSeconds: 15
            });
            define('jquery', function () { return jQuery; });
            define('knockout', ko);
        </script>

        @RenderSection("scripts", required: false)
    </body>
</html>
