@using System.Web.Mvc.Html
@{
    Layout = null;
    var splash = Server.MapPath("~/_splash.html");
    var splashHtml = "";
    if (File.Exists(splash))
    {
        splashHtml = (File.ReadAllText(splash));
    }
}
<!DOCTYPE html>
<html lang="en" class="no-js">
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>Rx Developer | Dashboard</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/assets/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->


    <!-- BEGIN THEME STYLES -->
    <link href="/Content/__css.bundle.dev.min.css" rel="stylesheet" type="text/css" />
    <link href="/assets/admin/layout/css/layout.css" rel="stylesheet" type="text/css" />
    <link href="/content/darkblue.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="/content/durandal.css" rel="stylesheet" type="text/css" id="style_color"/>
    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-md page-header-fixed page-quick-sidebar-over-content page-sidebar-closed-hide-logo ">
    @Html.Partial("_header")
    <div class="clearfix">
    </div>
    <!-- BEGIN CONTAINER -->
<div id="applicationHost" class="page-container">
    @if (!string.IsNullOrWhiteSpace(splashHtml))
    {
        @Html.Raw(splashHtml)
    }
    else
    {
        @Html.Partial("_splash")
    }

</div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <!-- END FOOTER -->


    <script src="/scripts/__vendor.dev.min.js" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="/assets/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script src="/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="/assets/global/scripts/metronic.js" type="text/javascript"></script>
    <script src="/assets/admin/layout/scripts/layout.js" type="text/javascript"></script>
    <script src="/assets/admin/layout/scripts/quick-sidebar.js" type="text/javascript"></script>
<!-- END PAGE LEVEL SCRIPTS -->
    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core componets
            Layout.init(); // init layout
            QuickSidebar.init(); // init quick sidebar
        });
    </script>
    @if (HttpContext.Current.IsDebuggingEnabled)
    {
        <script src="~/SphApp/objectbuilders.js"></script>
        <script src="/Scripts/__core.js"></script>
        <script src="/SphApp/schemas/__domain.js"></script>
        <script src="/SphApp/prototypes/prototypes.js"></script>
        <script src="/SphApp/partial/__partial.js"></script>
        <script src="/kendo/js/kendo.custom.min.js"></script>
    }
    else
    {
        <script src="/Scripts/__rx.min.js"></script>
    }

    <script type="text/javascript" src="/sph/entitydefinition/schemas"></script>
    <script type="text/javascript" src="/Scripts/require.js" data-main="SphApp/main"></script>

    <!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>
