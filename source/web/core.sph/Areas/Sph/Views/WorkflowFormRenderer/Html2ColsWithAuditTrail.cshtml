@using System.Web.Mvc.Html
@model Bespoke.Sph.Web.ViewModels.FormRendererViewModel

@{

    Layout = null;
    var form = Model.WorkflowForm;
    var caption = string.IsNullOrWhiteSpace(form.Caption) ?form.Name : form.Caption;

}

<h1 data-i18n="@caption">@caption</h1>
@Html.Partial("_ErrorList")
<div class="row">
    <div class="col-sm-8">
        @Html.Partial("_FormContent")
    </div>
</div>