@model Bespoke.Sph.Domain.ImageElement
@if (string.IsNullOrWhiteSpace(Model.Enable))
{
    Model.Enable = "true";
}@{
    var path = string.Format("'/sph/image/store/' + {0}()", this.Model.Path);
 }
<div class="form-group">
    <label class="col-md-@(Model.LabelColMd ?? 4)">@Model.Label</label>
    <img alt="@Model.Label" title="@Model.Tooltip" width="@Model.Width" height="@Model.Height" data-bind="attr : {'src':@Html.Raw(path)}, visible:@Html.Raw(Model.Visible)" />
</div>