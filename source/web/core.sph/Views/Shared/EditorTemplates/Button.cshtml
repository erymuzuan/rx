@model Bespoke.Sph.Domain.Button
@if (string.IsNullOrWhiteSpace(Model.Enable))
{
    Model.Enable = "true";
}
@{
    var command = Model.UseClick ? "click" : "command";
}
<button data-bind="@command : $root.@Model.CommandName, visible :@Model.Visible, enable: @Model.Enable" class="@Model.CssClass">@Model.Label</button>
