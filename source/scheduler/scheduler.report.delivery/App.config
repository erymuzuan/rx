          <list element-type="Bespoke.Sph.Domain.ILogger, domain.sph">
            <object type="Bespoke.Sph.ElasticSearchLogger.Logger, elasticsearch.logger" />
            <object type="web.console.logger.Logger, web.console.logger" />
