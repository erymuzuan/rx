@using Bespoke.Sph.Domain
@using Bespoke.Sph.FormCompilers.DurandalJs
@using Bespoke.Sph.FormCompilers.DurandalJs.FormElements
@inherits Template<DateTimePickerCompiler>
@{
    var element = Model.Element;
    var required = element.IsRequired ? "required" : null;
}
<input @required class="@(element.CssClass + " form-control "+ element.Size)"
       data-bind="@Raw(Model.GetKnockoutBindingExpression())" id="@element.ElementId" type="text"
       name="@element.Path" />
