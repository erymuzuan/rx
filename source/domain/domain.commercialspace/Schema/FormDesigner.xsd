					</xs:complexType>
				</xs:element>
				<xs:element name="CustomFieldCollection">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="CustomField" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="FormDesign"/>
			</xs:all>
			<xs:attribute name="ComplaintTemplateId" type="xs:int" use="required"/>
			<xs:attribute name="Name" type="xs:string" use="required"/>
			<xs:attribute name="Description" type="xs:string" use="required"/>
			<xs:attribute name="IsActive" type="xs:boolean" use="required"/>
			<xs:attribute name="Category" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
